{
  "mcpServers": {
    "angular-cli": {
      "command": "npx",
      "args": ["-y", "@angular/cli", "mcp"]
    },
    "primeng": {
      "command": "npx",
      "args": ["-y", "@primeng/mcp"]
    },
    "ag-mcp": {
      "command": "npx",
      "args": ["-y", "ag-mcp"]
    },
    "nx-mcp": {
      "command": "npx",
      "args": ["nx-mcp@latest"]
    },
    "react-docs": {
      "command": "npx",
      "args": ["-y", "react-docs-mcp"]
    },
    "figma": {
      "url": "https://mcp.figma.com/mcp",
      "type": "http"
    },
    "figma-desktop": {
      "url": "http://127.0.0.1:3845/mcp",
      "type": "http"
    },
    "nats": {
      "command": "npx",
      "args": ["-y", "nats-mcp-server"],
      "env": {
        "NATS_URL": "nats://localhost:4222"
      }
    }
  }
}
