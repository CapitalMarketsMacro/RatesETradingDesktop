{
  "app": {
    "name": "Rates E-Trading Desktop",
    "version": "1.0.0",
    "environment": "staging"
  },
  "api": {
    "baseUrl": "https://staging-api.rates-trading.com/api",
    "timeout": 30000,
    "retryAttempts": 3
  },
  "transport": {
    "type": "solace",
    "solace": {
      "url": "wss://staging-solace.rates-trading.com:443",
      "vpnName": "staging-vpn",
      "userName": "staging-user",
      "clientName": "rates-desktop-staging"
    }
  },
  "marketData": {
    "updateInterval": 500,
    "maxHistorySize": 2000,
    "symbols": ["2Y", "3Y", "5Y", "7Y", "10Y", "30Y"]
  },
  "trading": {
    "enabled": true,
    "maxOrderSize": 5000000,
    "defaultCurrency": "USD"
  },
  "features": {
    "advancedCharts": true,
    "realTimeUpdates": true,
    "orderManagement": true
  },
  "logging": {
    "level": "info",
    "enableConsole": true,
    "enableRemote": true
  },
  "ui": {
    "theme": "auto",
    "defaultPageSize": 50,
    "refreshInterval": 3000
  }
}
