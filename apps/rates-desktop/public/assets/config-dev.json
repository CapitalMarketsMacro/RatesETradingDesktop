{
  "app": {
    "name": "Rates E-Trading Desktop",
    "version": "1.0.0",
    "environment": "dev"
  },
  "api": {
    "baseUrl": "http://localhost:3000/api",
    "timeout": 30000,
    "retryAttempts": 3
  },
  "transport": {
    "type": "solace",
    "solace": {
      "url": "ws://localhost:8008",
      "vpnName": "dev-vpn",
      "userName": "dev-user",
      "clientName": "rates-desktop-dev"
    }
  },
  "marketData": {
    "updateInterval": 1000,
    "maxHistorySize": 1000,
    "symbols": ["2Y", "3Y", "5Y", "7Y", "10Y", "30Y"]
  },
  "trading": {
    "enabled": true,
    "maxOrderSize": 1000000,
    "defaultCurrency": "USD"
  },
  "features": {
    "advancedCharts": true,
    "realTimeUpdates": true,
    "orderManagement": true
  },
  "logging": {
    "level": "debug",
    "enableConsole": true,
    "enableRemote": false
  },
  "ui": {
    "theme": "light",
    "defaultPageSize": 20,
    "refreshInterval": 5000
  }
}
